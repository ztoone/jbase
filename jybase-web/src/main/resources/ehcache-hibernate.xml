    <cache name="com.jingyou.jybase.framework.core.bean.sys.BtnBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
    <cache name="com.jingyou.jybase.framework.core.bean.sys.ResourceBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
    <cache name="com.jingyou.jybase.framework.core.bean.sys.RoleBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
    <cache name="com.jingyou.jybase.framework.core.bean.sys.RoleResBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
    <cache name="com.jingyou.jybase.framework.core.bean.sys.FilterBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
    <cache name="com.jingyou.jybase.framework.core.bean.sys.FilterItemBean" maxEntriesLocalHeap="100" eternal="false" overflowToDisk="true" maxEntriesLocalDisk="100000" />
</ehcache>
